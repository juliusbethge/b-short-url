{
  "hosting": {
    "public": "build",
    "redirects": [
      {
        "regex": "^/(?P<shortHash>[A-Za-z%5Cd+/]+)$",
        "destination": "https://europe-west1-b-short-url.cloudfunctions.net/openLink/:shortHash",
        "type": 301
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}